<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Titile</th>
      <th>User</th>
      <th>Restaurant</th>
      <th>Content</th>
      <th>Time zone</th>
      <th>Amount</th>
      <th>Total eval</th>
      <th>Quality eval</th>
      <th>Service eval</th>
      <th>Atomos eval</th>
      <th>Drink eval</th>
      <th>Partner eval</th>
      <th>Visit date</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.titile %></td>
        <td><%= post.user_id %></td>
        <td><%= post.restaurant_id %></td>
        <td><%= post.content %></td>
        <td><%= post.time_zone %></td>
        <td><%= post.amount %></td>
        <td><%= post.total_eval %></td>
        <td><%= post.quality_eval %></td>
        <td><%= post.service_eval %></td>
        <td><%= post.atomos_eval %></td>
        <td><%= post.drink_eval %></td>
        <td><%= post.partner_eval %></td>
        <td><%= post.visit_date %></td>
        <td><%= post.url %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
